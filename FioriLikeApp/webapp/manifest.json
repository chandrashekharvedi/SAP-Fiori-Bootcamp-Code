{
    "_version": "1.43.0",
    "sap.app":{
        "id": "ey.sd.products",
        "type": "application",
        "ach": "FIN-AC-AP",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDesciption}}",
        "i18n": "i18n/i18n.properties"
    },
    "sap.ui":{
        "deviceTypes": {
            "desktop": true,
            "phone": true,
            "tablet":true 
        },
        "supportedThemes": ["sap_fiori_3","sap_fiori_3_dark"],
        "fullWidth": true,
        "technology": "UI5"
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.100",
            "libs": {
                "sap.ui.core": { "minVersion": "1.99" },
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "rootView":{
            "id": "idRootView",
            "viewName": "ey.sd.products.view.App",
            "type": "XML"
        },
        "routing": {
            "config":{
                "routerClass": "sap.m.routing.Router",
                "viewPath": "ey.sd.products.view",
                "viewType": "XML",
                "controlId": "appCon"
            },
            "routes":[
                {
                    "pattern": "",
                    "name": "spiderman",
                    "target": ["tom", "empty"]
                },
                {
                    "pattern": "fruits/{fruitId}:?query:",
                    "name": "fruits",
                    "target": ["tom", "jerry"]
                },
                {
                    "pattern": "supplier/{supplierId}",
                    "name": "supplier",
                    "target": ["tom", "supplier"]
                }
            ],
            "targets": {
                "tom": {
                    "viewName": "View1",
                    "controlAggregation": "masterPages",
                    "viewId": "idView1"
                },
                "jerry": {
                    "viewId": "idView2",
                    "viewName": "View2",
                    "controlAggregation": "detailPages"
                },
                "empty": {
                    "viewId": "idEmptyView",
                    "viewName": "Empty",
                    "controlAggregation": "detailPages"
                },
                "supplier": {
                    "viewId": "idSupplierView",
                    "viewName": "Supplier",
                    "controlAggregation": "detailPages"
                }
            }
        },
        "models": {
            "":{
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/mockdata/fruits.json",
                "preload": false,
                "settings": {}
            },
            "UIprops": {
                "type": "sap.ui.model.json.JSONModel",
                "preload": false
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "preload": true,
                "settings": {
                    "bundleUrl": "i18n/i18n.properties"
                }
            }
        }
        
    }
}